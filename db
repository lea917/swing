    DefaultModel lea=new DefaultModel();
private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         

       
         jTextField1.setText("");
         jTextField2.setText("");
         JOptionPane.showMessageDialog(rootPane,genderGroup.getSelection().getActionCommand());
         JLabel5.setText(jComboBox1.get selectedItem());
         JOptionPane.showMessageDialog("Date"+jComboBox1.getSelectedItem().to int());
         JLabel5.setText(jComboBox2.get selectedItem());
         JOptionPane.showMessageDialog("Month"+jComboBox2.getSelectedItem().to istring());
         JLabel5.setText(jComboBox3.get selectedItem());
         JOptionPane.showMessageDialog("Year"+jComboBox3.getSelectedItem().to int());
         if(jCheckBox1.is selected())
         {
            JOptionPane.showMessageDialog(rootpane,jCheckBox1.getText());  
         }
          jList1.setModel(lea);
         lea.addElement(jTextArea1.getText().trim());
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        lea.resetElement(jList1.getSelecctedIIndex());
        
    }                                        
